{
  "name": "nodart",
  "version": "1.0.3",
  "description": "typescript backend framework",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodart-labs/nodart.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --loader ts-node/esm testapp",
    "build": "concurrently \"tsc --build\" \"npm run post-build\"",
    "post-build": "node post-build.js",
    "clean": "tsc --build --clean",
    "test": "tests"
  },
  "author": "ArGab",
  "license": "MIT",
  "dependencies": {
    "ts-node": "^10.8.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "concurrently": "^7.3.0",
    "copy-dir": "^1.3.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.6.4"
  },
  "keywords": [
    "ts framework",
    "typescript framework",
    "node.js framework"
  ]
}
