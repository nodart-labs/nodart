{
  "name": "nodart",
  "version": "2.0.0",
  "description": "typescript backend framework",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodart-labs/nodart.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --loader ts-node/esm tests/app",
    "build": "concurrently \"tsc --build\" \"npm run post-build\"",
    "post-build": "node post-build.js",
    "clean": "tsc --build --clean",
    "test": "tests"
  },
  "author": "ArGab",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^7.6.2",
    "client-sessions": "^0.8.0",
    "knex": "^2.2.0",
    "mysql": "^2.18.1",
    "nunjucks": "^3.2.3",
    "pg": "^8.8.0",
    "sqlite3": "^5.0.11",
    "ts-node": "^10.8.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "concurrently": "^7.3.0",
    "copy-dir": "^1.3.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.6.4"
  },
  "types": "index.d.ts",
  "keywords": [
    "ts framework",
    "typescript framework",
    "node.js framework"
  ]
}
