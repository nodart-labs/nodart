{
  "name": "nodart",
  "version": "5.2.0",
  "description": "A complete framework for creating microservices and large-scale server-side applications for businesses",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodart-labs/nodart.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --loader ts-node/esm tests/app",
    "build": "npm run pre-build && tsc --build && npm run post-build",
    "pre-build": "node pre-build.js",
    "post-build": "node post-build.js",
    "clean": "tsc --build --clean",
    "lint": "eslint . --ext .js,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "migrate@sources:up": "node cmd migrate all-source-up",
    "migrate@sources:down": "node cmd migrate all-source-down",
    "seed@sources": "node cmd seed all-source-run"
  },
  "author": "ArGab",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^8.2.0",
    "busboy": "^1.6.0",
    "client-sessions": "^0.8.0",
    "decompress": "^4.2.1",
    "knex": "^2.4.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.2.0",
    "nunjucks": "^3.2.3",
    "oracledb": "^5.5.0",
    "pg": "^8.10.0",
    "sqlite3": "^5.1.6",
    "tedious": "^15.1.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "copy-dir": "^1.3.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.7",
    "prettier-eslint": "^15.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "keywords": [
    "framework",
    "ts",
    "ECMAScript",
    "JavaScript",
    "typescript",
    "back-end",
    "backend",
    "server-side",
    "server",
    "MVC",
    "OOP",
    "node.js"
  ],
  "bin": "bin/index.js"
}
