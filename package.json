{
  "name": "nodart",
  "version": "3.2.0",
  "description": "typescript backend framework",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodart-labs/nodart.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --loader ts-node/esm tests/app",
    "build": "npm run pre-build && tsc --build && npm run post-build",
    "pre-build": "node pre-build.js",
    "post-build": "node post-build.js",
    "clean": "tsc --build --clean",
    "test": "tests"
  },
  "author": "ArGab",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^7.6.2",
    "client-sessions": "^0.8.0",
    "decompress": "^4.2.1",
    "knex": "^2.2.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "nunjucks": "^3.2.3",
    "oracledb": "^5.4.0",
    "pg": "^8.8.0",
    "sqlite3": "^5.0.11",
    "tedious": "^15.1.0",
    "ts-node": "^10.8.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "copy-dir": "^1.3.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.6.4"
  },
  "keywords": [
    "ts framework",
    "typescript framework",
    "node.js framework"
